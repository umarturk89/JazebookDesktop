                        </StackPanel>
                    </TabItem.Header>
                    <TabItem.Content>
                        <DockPanel x:Name="NotificationsTab_DockPanel" Margin="0,0,0,0" Background="#efefef" Width="280" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
