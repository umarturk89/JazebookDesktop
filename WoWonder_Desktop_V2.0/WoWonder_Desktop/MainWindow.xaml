                            <RichTextBox  FontFamily="{StaticResource LatoRegular}"
                                          Foreground="White" BorderBrush="{Binding Color_box_message}" Background="{Binding Color_box_message}" IsReadOnly="True" IsDocumentEnabled="True"  controls:RTBNavigationService.Content="{Binding Mes_Text}"  >
