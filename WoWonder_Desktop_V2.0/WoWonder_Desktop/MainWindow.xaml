                                                <ComboBoxItem x:Name="Item_br" x:Uid="ComboBoxItem_br" Content="Portuguese (pt)" Tag="pt-BR"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_ru" x:Uid="ComboBoxItem_ru" Content="Russian (ru)" Tag="ru-RU"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_id" x:Uid="ComboBoxItem_id" Content="Indonesian (id)" Tag="id-ID"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_es" x:Uid="ComboBoxItem_es" Content="Spanish (es)" Tag="es-ES"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_tr" x:Uid="ComboBoxItem_tr" Content="Turkish (tr)" Tag="tr-TR"  Foreground="Black"  FontWeight="Normal"/>
                                            </ComboBox>

                                        </StackPanel>

                                        <StackPanel Orientation="Horizontal">
                                            <!--text -->
                                            <Label x:Name="Lbl_Night_Mode" Margin="4 5 15 0" FontWeight="Normal" FontSize="13" Foreground="Black"  Content="{x:Static properties:LocalResources.label2_Night_Mode}" HorizontalAlignment="Left"/>

                                            <ToggleButton x:Name="Btn_SwitchDarkMode" Margin="0 5 0 0" Style="{StaticResource MaterialDesignSwitchDarkToggleButton}" VerticalAlignment="Center"
