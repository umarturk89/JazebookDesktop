                                        </Hyperlink>
                                        </TextBlock>

                                    </StackPanel>
                                </Grid>
                                <!-- Advanced Settings -->
                                <Grid x:Name="S_Advanced_Settings" Margin="0,3,0,0"  Background="White" Grid.Row="3">
