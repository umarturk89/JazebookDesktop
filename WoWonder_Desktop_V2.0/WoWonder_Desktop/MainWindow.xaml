                                        </ListBox.ItemContainerStyle>
                                    </ListBox>
                                </Grid>

                                <!--pro Users-->
                                <Grid x:Name="proUsers" Margin="0,3,0,0"  Background="White" Grid.Row="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Grid x:Name="Title_proUsers" Grid.Column="0" Margin="0,6,0,0" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="30" />
                                        </Grid.ColumnDefinitions>

                                        <Label x:Name="Lbl_proUsers" Margin="4,2,0,0" Grid.Row="0"
                                               FontWeight="Normal" FontFamily="{StaticResource LatoBold}"
                                               Content="{x:Static properties:LocalResources.label_proUsers}" Foreground="{StaticResource WownderDefaultBrush}" 
                                               HorizontalAlignment="Left"  FontSize="12" />

                                        <!--icon Notifications-->
                                        <materialDesign:PackIcon  Kind="AccountMultiple" 
                                                                  Grid.Row="0"  Margin="2,5,0,0"  Grid.Column="1"
                                                                  VerticalAlignment="Top" 
                                                                  Foreground="#A7A7A7"  Width="18" Height="18" 
                                                                  />
                                    </Grid>

                                    <Grid Grid.Column="0" Margin="0,1,0,0" Grid.Row="1">

                                        <ListBox Grid.Row="0" Margin="6,0,0,0" Panel.ZIndex="2" Height="60" 
                                                 Background="{x:Null}" x:Name="ProUsersList" SelectionChanged="ProUsersList_SelectionChanged"
                                                 BorderBrush="{x:Null}" ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                 ScrollViewer.VerticalScrollBarVisibility="Disabled" >
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Border>
                                                        <Grid>
                                                            <!--Image Pro Users-->
                                                            <Image x:Name="ProAvatarofUsers" Margin="-3,0,-3,0" ToolTip="{Binding P_username}" Source="{Binding P_avatar}" Stretch="UniformToFill" Width="32" Height="32">
                                                                <Image.Clip>
                                                                    <EllipseGeometry Center="16,16" RadiusX="16" RadiusY="16" />
                                                                </Image.Clip>
                                                            </Image>
                                                        </Grid>
                                                    </Border>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                            <ListBox.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <WrapPanel/>
                                                </ItemsPanelTemplate>
                                            </ListBox.ItemsPanel>

                                        </ListBox>
                                    </Grid>
                                </Grid>

                                <!--HashTag-->
                                <Grid x:Name="HashTag" Margin="0,3,0,0"  Background="White" Grid.Row="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Grid x:Name="Title_Trending_HashTags" Grid.Column="0" Margin="0,6,0,0" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="30" />
                                        </Grid.ColumnDefinitions>
                                        <Label x:Name="Lbl_Trending_HashTags" Margin="4,2,0,0" Grid.Row="0" FontWeight="Normal" 
                                               FontFamily="{StaticResource LatoBold}" Content="{x:Static properties:LocalResources.label_Trending_HashTags}" 
                                               Foreground="{StaticResource WownderDefaultBrush}" HorizontalAlignment="Left"
                                               FontSize="12" />

                                        <!--icon Notifications-->
                                        <materialDesign:PackIcon  Kind="Flash" 
                                                                  Grid.Row="0"  Margin="2,5,0,0"  Grid.Column="1"                                                                   VerticalAlignment="Top" 
                                                                  Foreground="#A7A7A7"  Width="18" Height="18" 
                                                                  />

                                    </Grid>
                                    <Border x:Name="Hashtag_list_Border" BorderBrush="White" BorderThickness="0" Background="#FCFCFC" Grid.Row="1" Padding="2">
                                        <ListBox x:Name="Hashtag_list" Margin="5 0 5 0" SelectionChanged="Hashtag_list_SelectionChanged">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate >
                                                    <StackPanel>
                                                        <Label x:Name="Hashtag_Label" Foreground="{ Binding S_Color_Foreground}" Content="{Binding T_tag}" />
                                                    </StackPanel>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>

                                        </ListBox>
                                    </Border>

                                </Grid>
                            </Grid>
                        </DockPanel>
                    </TabItem.Content>
                </TabItem>

                <!-- ######### Settings Tab ######### -->
                <TabItem Name="SettingsTab">
                    <TabItem.Header>
                        <StackPanel>
                            <materialDesign:PackIcon Kind="Settings" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" Height="22" Width="22" />
