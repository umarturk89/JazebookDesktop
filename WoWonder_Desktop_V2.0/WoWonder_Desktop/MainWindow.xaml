                                                <ComboBoxItem x:Name="Item_en" x:Uid="ComboBoxItem_en" Content="English (en)" Tag="en-US" Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_ar" x:Uid="ComboBoxItem_ar" Selector.IsSelected="True" Content="Arabic (ar)" Tag="ar"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_de" x:Uid="ComboBoxItem_de" Content="German (de)" Tag="de-DE" Foreground="Black"  FontWeight="Normal" />
                                                <ComboBoxItem x:Name="Item_nl" x:Uid="ComboBoxItem_nl" Content="Dutch (nl)" Tag="nl-NL"  Foreground="Black"  FontWeight="Normal"/>
                                                <ComboBoxItem x:Name="Item_fr" x:Uid="ComboBoxItem_fr" Content="French (fr)" Tag="fr-FR" Foreground="Black" FontWeight="Normal"/>
