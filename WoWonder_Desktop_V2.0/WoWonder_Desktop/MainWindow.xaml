                                                                <Rectangle RadiusX="7" RadiusY="7" Width="45" Height="45">
                                                                    <Rectangle.Fill>
                                                                        <ImageBrush x:Name="myImage" ImageSource="{Binding N_avatar}"/>
                                                                    </Rectangle.Fill>
                                                                </Rectangle>

                                                                <StackPanel Background="{Binding S_Color_Background}" HorizontalAlignment="Stretch" Width="125" Orientation="Vertical" >
                                                                    <!-- text username-->
                                                                    <StackPanel Background="{Binding S_Color_Background}" HorizontalAlignment="Stretch" Orientation="Horizontal" Grid.Column="1" Grid.Row="0">
                                                                        <Label Foreground="{Binding S_Color_Foreground}" Content="{Binding N_username}" FontWeight="ExtraBold" Margin="5 0 0 0" />
                                                                    </StackPanel>
                                                                    <!-- text Notifications-->
                                                                    <StackPanel Background="{Binding S_Color_Background}" HorizontalAlignment="Stretch" Orientation="Horizontal" Margin="0 5 0 0" Grid.Column="1" Grid.Row="1">
                                                                        <materialDesign:PackIcon
                                                                            HorizontalAlignment="Left" VerticalAlignment="Center"
                                                                            Margin="5 -5 0 0" 
                                                                            Kind="{Binding N_Type_icon}" 
                                                                            Width ="15" Height="15" Foreground="{Binding N_Color_icon}" />
                                                                        <Label Foreground="{Binding S_Color_Foreground}" Content="{Binding N_type_text}"  Margin="3 -5 0 0" FontSize="10"></Label>
                                                                    </StackPanel>
                                                                </StackPanel>
                                                                <!-- text time-->
                                                                <StackPanel Background="{Binding S_Color_Background}"  Grid.Column="2" Grid.Row="0"  Margin="15 8 0 0" >
                                                                    <Label Content="{Binding N_time_text_string}"
                                                                           FontSize="12" HorizontalAlignment="Right"
                                                                           VerticalAlignment="Top" Foreground="{Binding S_Color_Foreground}" >
                                                                    </Label>
                                                                </StackPanel>
                                                            </StackPanel>
                                                            <ControlTemplate.Triggers>
                                                                <Trigger Property="IsMouseOver" Value="False">
                                                                    <!--<Setter TargetName="Border" Property="Background" Value="#f8f8f8"/>-->
                                                                </Trigger>
                                                            </ControlTemplate.Triggers>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                                <Style.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
