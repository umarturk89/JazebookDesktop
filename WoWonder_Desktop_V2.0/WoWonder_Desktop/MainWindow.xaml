                                               FontFamily="{StaticResource LatoBold}" Content="{x:Static properties:LocalResources.label_Notifications}" 
                                               Foreground="{StaticResource WownderDefaultBrush}" HorizontalAlignment="Left"
                                               FontSize="12" />

